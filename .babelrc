{
  "presets": [
    "@babel/preset-env",
    "@babel/react"
  ],
  "plugins": [
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }],
    ["@babel/plugin-proposal-decorators", {"legacy": true}],
    "@babel/plugin-proposal-class-properties",
    "@babel/transform-runtime",
    "react-hot-loader/babel"
  ]
}
